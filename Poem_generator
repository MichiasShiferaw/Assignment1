def poem_generator():
    name1 = input('Please enter a first name ')  #Prompt user for the name
    city1 = input('Please enter your city of birth')   #Prompt user for a city

    print('Look there goes the balloon! Imagine me floating to the Moon' '\n' 'And let me swing among the stars',
          'Let me see what ', name1, ' is doing on ', city1, ' and Mars')  #prints a poem that includes that name and city provided
    return
