def bibformat(author, title, city, publisher, year):
    author1 = author #assign variable to each inputted value
    year1 = year
    title1 = title
    city1 = city
    publisher1 = publisher
    all_together = (author1, year1, title1, city1, publisher1)  #place all the values (in the correct order) to a new variable
    return all_together  #output the result
